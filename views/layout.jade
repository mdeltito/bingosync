doctype 5
html(lang="en")
  head
  title= "BINGOSYNC"
  !=css('bootstrap/bootstrap')
  !=css('bingosync')
  link(rel="stylesheet", type="text/css", href="/css/darkstrap.css")
  link(rel="stylesheet", type="text/css", href="/css/jquery.gritter.css")

  body
    div.navbar
      div.navbar-inner
        div.container
          a(href='#').brand.pull-left
            | BINGOSYNC&nbsp;
            span.label.label-info v1.1
          block nav
    div#main.container-fluid
      div.row-fluid
        .span9
          block content
        .span3
          block sidebar
    footer
      block footer

    script(type="text/javascript", src="//cdnjs.cloudflare.com/ajax/libs/underscore.js/1.4.4/underscore-min.js")
    script(type="text/javascript", src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js")
    script(type="text/javascript", src="/socket.io/socket.io.js")
    script(type="text/javascript", src="/js/jquery.gritter.min.js")
    script(type="text/javascript", src="/js/bootstrap.min.js")
    !=js('client')
